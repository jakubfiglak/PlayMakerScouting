{
  "name": "playmaker-pro-scouting",
  "version": "1.0.0",
  "description": "PlaymakerPro Scouting App",
  "engines": {
    "node": "16.x",
    "npm": "6.x"
  },
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test-coverage": "jest --watchAll --verbose --runInBand --detectOpenHandles --coverage",
    "test": "jest --watch --verbose --runInBand --detectOpenHandles",
    "test-one": "jest --watch --detectOpenHandles",
    "seed-users": "node scripts/seed-users",
    "seed-clubs": "node scripts/seed-clubs-from-csv",
    "seed-players": "node scripts/seed-players-from-csv",
    "delete-players": "node scripts/delete-generated-players",
    "seed-matches": "node scripts/seed-matches-from-csv",
    "delete-matches": "node scripts/delete-generated-matches",
    "seed-notes": "node scripts/seed-notes-from-csv",
    "delete-notes": "node scripts/delete-generated-notes",
    "seed-ratings": "node scripts/seed-ratings -create",
    "destroy-ratings": "node scripts/seed-ratings -destroy",
    "seed-reportTemplates": "node scripts/seed-reportTemplates -create",
    "destroy-reportTemplates": "node scripts/seed-reportTemplates -destroy",
    "seed-reportBackgroundImages": "node scripts/seed-reportBackgroundImages -create",
    "destroy-reportBackgroundImages": "node scripts/seed-reportBackgroundImages -destroy",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client & migrate-mongo up"
  },
  "author": "Jakub Figlak",
  "license": "ISC",
  "dependencies": {
    "@meanie/mongoose-to-json": "^2.5.0",
    "@sendgrid/mail": "^7.4.2",
    "@sentry/node": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "fast-csv": "^4.3.6",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^8.1.4",
    "mongo-sanitize": "^1.1.0",
    "mongoose": "^5.9.13",
    "mongoose-autopopulate": "^0.13.0",
    "mongoose-paginate-v2": "^1.3.9",
    "mongoose-sequence": "^5.3.1",
    "morgan": "^1.10.0",
    "node-geocoder": "^3.27.0",
    "nodemailer": "^6.4.6",
    "slugify": "^1.4.0",
    "validator": "^13.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "concurrently": "^5.3.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^5.4.0",
    "jest": "^26.6.0",
    "jest-in-case": "^1.0.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  }
}
